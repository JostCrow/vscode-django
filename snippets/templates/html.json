        ]
    },

    "endregroup": {
        "description": "{% endregroup %}",
        "prefix": "endregroup",
        "body": [
            "{% endregroup %}"
        ]
    },

    "resetcycle": {
        "description": "{% resetcycle %}",
        "prefix": "resetcycle",
        "body": [
            "{% resetcycle $1 %}$0",
            "{% endresetcycle %}"
        ]
    },

    "endresetcycle": {
        "description": "{% endresetcycle %}",
        "prefix": "endresetcycle",
        "body": [
            "{% endresetcycle %}"
        ]
    },

    "spaceless": {
        "description": "{% spaceless %}",
        "prefix": "spaceless",
        "body": [
            "{% spaceless $1 %}$0",
            "{% endspaceless %}"
        ]
    },

    "endspaceless": {
        "description": "{% endspaceless %}",
        "prefix": "endspaceless",
        "body": [
            "{% endspaceless %}"
        ]
    },

    "templatetag": {
        "description": "{% templatetag %}",
        "prefix": "templatetag",
        "body": [
            "{% templatetag $1 %}",
        ]
    },

    "url": {
        "description": "{% url %}",
        "prefix": "url",
        "body": [
            "{% url '$1' %}"
        ]
    },

    "urlslug": {
        "description": "{% url %}",
        "prefix": "urlslug",
        "body": [
            "{% url '$1' slug=$2 %}"
        ]
    },

    "urlpk": {
        "description": "{% url %}",
        "prefix": "urlpk",
        "body": [
            "{% url '$1' pk=$2 %}"
        ]
    },

    "verbatim": {
        "description": "{% verbatim %}",
        "prefix": "verbatim",
        "body": [
            "{% verbatim $1 %}$0",
            "{% endverbatim %}"
        ]
    },

    "endverbatim": {
        "description": "{% endverbatim %}",
        "prefix": "endverbatim",
        "body": [
            "{% endverbatim %}"
        ]
    },

    "widthratio": {
        "description": "{% widthratio %}",
        "prefix": "widthratio",
        "body": [
            "{% widthratio $1 %}$0",
            "{% endwidthratio %}"
        ]
    },

    "endwidthratio": {
        "description": "{% endwidthratio %}",
        "prefix": "endwidthratio",
        "body": [
