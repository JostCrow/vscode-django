        ]
    },

    "include": {
        "description": "{% include %}",
        "prefix": "include",
        "body": [
            "{% include $1 %}"
        ]
    },

    "load": {
        "description": "{% load tags %}",
        "prefix": "load",
        "body": [
            "{% load ${1|static,i18n,cache,l10n,tz,humanize,flatpages,admin_list,admin_modify,admin_static,admin_urls,base,log|} %}"
        ]
    },

    "lorem": {
        "description": "{% lorem %}",
        "prefix": "lorem",
        "body": [
            "{% lorem $1 %}$0",
            "{% endlorem %}"
        ]
    },

    "endlorem": {
        "description": "{% endlorem %}",
        "prefix": "endlorem",
        "body": [
            "{% endlorem %}"
        ]
    },

